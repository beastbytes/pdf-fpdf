{
    "name": "beastbytes/pdf-fpdf",
    "description": "PDF generator using the tFPDF library",
    "keywords": [
        "FPDF",
        "PDF"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Chris Yates",
            "email": "chris.l.yates@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "beastbytes/pdf": "dev-master",
        "setasign/tfpdf": "^1.0",
        "yiisoft/aliases": "^3.0",
        "yiisoft/arrays": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "roave/infection-static-analysis-plugin": "^1.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.0",
        "yiisoft/test-support": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\PDF\\FPDF\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\PDF\\FPDF\\Tests\\": "tests"
        }
    }
}
